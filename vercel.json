{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/app/api/$1"
    },
    {
      "src": "/blog/(.*)",
      "dest": "/src/app/blog/$1"
    },
    {
      "src": "/about",
      "dest": "/src/app/about/page.tsx"
    },
    {
      "src": "/projects",
      "dest": "/src/app/projects/page.tsx"
    },
    {
      "src": "/",
      "dest": "/src/app/page.tsx"
    }
  ]
}